{
  "name": "firestore-storage",
  "version": "2.5.0",
  "description": "A typed wrapper around Firestore including a query-builder and an in-memory implementation for testing",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "keywords": [
    "firestore",
    "typescript",
    "memory",
    "querybuilder"
  ],
  "directories": {
    "test": "test",
    "lib": "src"
  },
  "files": [
    "dist/",
    "src/",
    "test/",
    ".editorconfig",
    "tsconfig.json",
    "README.md"
  ],
  "engines": {
    "node": ">=6.9.0"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha dist/test",
    "test:firestore": "yarn test --opts test/mocha.firestore.opts",
    "test:all": "yarn test && yarn test:firestore",
    "build": "tsc",
    "preversion": "git pull && yarn build && yarn test:all",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:freshfox/firestore-storage.git"
  },
  "author": "Dominic Bartl",
  "license": "MIT",
  "homepage": "https://github.com/freshfox/firestore-storage",
  "dependencies": {
    "@google-cloud/firestore": "^1.2.0",
    "firebase-admin": "^7.2.0",
    "inversify": "^4.3.0",
    "lodash": "^4.17.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^9.3.0",
    "@types/should": "^13.0.0",
    "mocha": "^5.0.0",
    "node-env-file": "^0.1.8",
    "should": "^13.2.3",
    "ts-node": "^5.0.1",
    "typescript": "^3.5.2"
  }
}
